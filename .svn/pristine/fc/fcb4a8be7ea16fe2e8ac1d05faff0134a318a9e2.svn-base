public class IssuerRelation {
	//relation_id,issuer_id,type_id,bd_id,issuer_merch_id
	private String relationId;
	private String typeId;
	private String bdId;
	private String issuerMerchId;
	public String getRelationId() {
		return relationId;
	}
	public void setRelationId(String relationId) {
		this.relationId = relationId;
	}
	public String getTypeId() {
		return typeId;
	}
	public void setTypeId(String typeId) {
		this.typeId = typeId;
	}
	public String getBdId() {
		return bdId;
	}
	public void setBdId(String bdId) {
		this.bdId = bdId;
	}
	public String getIssuerMerchId() {
		return issuerMerchId;
	}
	public void setIssuerMerchId(String issuerMerchId) {
		this.issuerMerchId = issuerMerchId;
	}
	
	
}
