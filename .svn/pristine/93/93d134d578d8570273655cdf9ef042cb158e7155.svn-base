package com.yunpay.wx.rep.card;

import com.yunpay.common.utils.EnumUtil.ResultCode;

public class CardRep extends AppBaseRep{
	
	
	//卡券Id
	private String card_id;
	
	public CardRep(String cardId){
		setReturn_code(ResultCode.SUCCESS.getCode());
		setCard_id(cardId);
	}
	
	public CardRep(String errCode,String errCodeDes){
		super(errCode,errCodeDes);
	}
	

	public String getCard_id() {
		return card_id;
	}

	public void setCard_id(String card_id) {
		this.card_id = card_id;
	}
	
	
}
